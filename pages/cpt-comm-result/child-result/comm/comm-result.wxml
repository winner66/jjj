      </view>
      <view class="weui-cell__fd">
        <view class="weui-label">元</view>
      </view>
    </view>
    <view class="weui-cell ">
      <view class="weui-cell__hd">
        <view class="weui-label">每月月供：</view>
      </view>
      <view class="weui-cell__bd">
