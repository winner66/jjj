<!--pages/cpt-comm-result/cpt-comm-result.wxml-->

<view class='result'>
<!-- 商贷 -->
<block wx:if="{{tab==0}}">
  <px-comm-result  info="{{tabdata}}"/>
</block>
  <!-- 税  住宅-->
  <block wx:elif="{{tab==1}}">
  <px-tax1-result  info="{{tabdata}}"/>
  </block>
    <!-- 税 非住宅 -->
  <block wx:elif="{{tab==4}}">
  <px-tax2-result  info="{{tabdata}}"/>
  </block>
  <!-- 公积金 -->
  <block wx:elif="{{tab==2}}">
  <px-fund-result  info="{{tabdata}}"/>
  </block>
  <block wx:else="{{tab==3}}">
  <px-compose-result  info="{{tabdata}}"/>
  </block> 

</view>